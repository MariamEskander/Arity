<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <ListPreference
        android:key="3d_shading"
        android:title="@string/shading3D"
        android:defaultValue="@string/shading_value_smooth"
        android:entries="@array/shading_entries"
        android:entryValues="@array/shading_values"
        />

    <ListPreference
        android:key="3d_resolution"
        android:title="@string/resolution3D"
        android:defaultValue="144"
        android:entries="@array/resolution_entries"
        android:entryValues="@array/resolution_values"
        />
    <SwitchPreference
        android:key="dyn_colors"
        android:title="@string/dyn_col"
        android:defaultValue="true"
        android:summary="Android 12+"
        />

</PreferenceScreen>
